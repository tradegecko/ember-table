{{#each cells as |api|}}
  {{#if hasBlock}}
    {{#if isHeader}}
      {{yield (hash
        columnValue=api.columnValue
        columnMeta=api.columnMeta
        sorts=api.sorts
        sendUpdateSort=api.sendUpdateSort
      )}}
    {{else}}
      {{yield (hash
        api=api
        cellValue=api.cellValue
        cellMeta=api.cellMeta
        columnValue=api.columnValue
        columnMeta=api.columnMeta
        rowValue=api.rowValue
        rowMeta=api.rowMeta
      )}}
    {{/if}}
  {{else if isHeader}}
    {{ember-th api=api}}
  {{else}}
    {{ember-td api=api}}
  {{/if}}
{{/each}}
